{
    "name": "yiddishe-kop/laravel-commerce",
    "description": "Simple commerce package for Laravel",
    "keywords": [
        "yiddishe-kop",
        "laravel-commerce"
    ],
    "homepage": "https://github.com/yiddishe-kop/laravel-commerce",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Yehuda Neufeld",
            "email": "yehuda@yiddishe-kop.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "YiddisheKop\\LaravelCommerce\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YiddisheKop\\LaravelCommerce\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "YiddisheKop\\LaravelCommerce\\LaravelCommerceServiceProvider"
            ],
            "aliases": {
                "LaravelCommerce": "YiddisheKop\\LaravelCommerce\\LaravelCommerceFacade"
            }
        }
    }
}
